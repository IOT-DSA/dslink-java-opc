{
  "name": "dslink-java-opc",
  "version": "0.2.4",
  "description": "A dslink for opc",
  "license": "GNU GPL",
  "main": "bin/dslink-java-opc",
  "author": {
    "name": "Daniel Shapiro",
    "email": "danielsspam1272@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-opc"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-opc/issues"
  },
  "configs": {
  	"name": {
      "type": "string",
      "default": "OPC"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "opc.Main"
    }
  }
}
